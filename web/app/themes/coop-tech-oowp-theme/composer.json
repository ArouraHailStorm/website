{
  "name": "outlandishideas/outlandish-wordpress-starter-theme",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme for working with Outlandish WordPress projects",
  "homepage": "http://outlandish.com/",
  "authors": [
    {
      "name": "Matthew Kendon",
      "email": "matt@benword.com",
      "homepage": "https://github.com/mattKendon"
    },
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "Outlandish\\MappingCoTech\\": "src/"
    }
  },
  "require": {
    "php": ">=5.4.45",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "phpmd/phpmd": "^2.4.2",
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.5",
    "behat/behat": "^3.2",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "behat/mink-selenium2-driver": "^1.3"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs src",
      "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming,unusedcode"
    ]
  },
  "config": {
    "platform": {
      "php": "5.4.45"
    }
  }
}
